<html>
  <head>
    <title>{{ title }}</title>
  </head>
  <body>
    <p>{{ text }}</p>
    {{ #for project in projects }}
      <a href="{{ project.url }}">{{ project.name }}</a>
      <p>{{ project.description }}</p>
    {{ /for }}
    {{ #if projects.size == 0 }}
      No projects
    {{ /if }}
  </body>
</html>
