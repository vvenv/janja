{
  "cSpell.words": [
    "elif",
    "endcall",
    "endcapture",
    "endcomment",
    "endfor",
    "endmacro",
    "endset",
    "endwith",
    "groupby",
    "janja",
    "liquidjs",
    "shiki",
    "urldecode",
    "urlencode",
    "Vitest",
    "vvenv"
  ],
  "prettier.enable": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "eslint.enable": true,
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "html",
    "markdown",
    "json",
    "css",
    "postcss"
  ],
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  }
}
