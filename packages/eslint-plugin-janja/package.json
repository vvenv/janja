{
  "name": "eslint-plugin-janja",
  "version": "1.1.0",
  "description": "ESLint plugin for Janja template formatting",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "vitest watch",
    "test": "vitest"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "janja",
    "template",
    "formatting"
  ],
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "dependencies": {
    "@eslint/core": "^1.0.0",
    "@eslint/plugin-kit": "^0.5.0",
    "janja": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.10.3",
    "@vitest/coverage-v8": "4.0.15",
    "@vitest/ui": "4.0.16",
    "eslint": "^9.39.2",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.3.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.16"
  }
}
